<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>PuppeteerSharpExtra6</Title>
    <Authors>Berstend, Fahad Altaf</Authors>
    <PackageProjectUrl>https://github.com/berstend/puppeteer-extra</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Plugins\ExtraStealth\Scripts\ChromeApp.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\Codec.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\ContentWindow.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\HardwareConcurrency.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\Language.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\LoadTimes.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\Outdimensions.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\Permissions.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\Plugin.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\Runtime.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\SCI.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\Stacktrace.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\Utils.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\Vendor.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\WebDriver.js" />
    <None Remove="Plugins\ExtraStealth\Scripts\WebGL.js" />
    <None Remove="Plugins\Recaptcha\Scripts\EnterRecaptchaCallBackScript.js" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\ChromeApp.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\Codec.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\ContentWindow.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\HardwareConcurrency.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\Language.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\LoadTimes.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\Outdimensions.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\Permissions.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\Plugin.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\Runtime.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\SCI.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\Stacktrace.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\Utils.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\Vendor.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\WebDriver.js" />
    <EmbeddedResource Include="Plugins\ExtraStealth\Scripts\WebGL.js" />
    <EmbeddedResource Include="Plugins\Recaptcha\Scripts\EnterRecaptchaCallBackScript.js" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="PuppeteerSharp" Version="8.0.0" />
    <PackageReference Include="RestSharp" Version="106.11.5" />
  </ItemGroup>

</Project>
